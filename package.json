    "@embroider/macros": "^1.16.12",
    "@embroider/vite": "^1.1.1",
    "@eslint/js": "^9.23.0",
    "@glimmer/component": "^1.0.0",
    "@glimmer/tracking": "^1.1.2",
    "@percy/cli": "^1.30.7",
    "@percy/ember": "^3.0.1",
    "@rollup/plugin-babel": "^6.0.4",
    "@sinonjs/fake-timers": "^7.1.2",
    "babel-plugin-ember-template-compilation": "^2.3.0",
    "concurrently": "^9.1.2",
