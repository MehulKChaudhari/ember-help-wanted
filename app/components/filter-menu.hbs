<div local-class="container">
  {{#each this.filterNames as |filterName|}}
    <LinkTo
      class="es-button"
      data-test-link={{filterName}}
      local-class="link"
      @model={{filterName}}
      @query={{hash query=""}}
      @route="issues"
    >
      {{filterName}}
    </LinkTo>
  {{/each}}
</div>
